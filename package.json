{
  "name": "corkscrew",
  "version": "1.0.0",
  "description": "MERN stack app for hospitality management",
  "main": "index.js",
  "dependencies": {
    "axios": "^0.20.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "install": "^0.13.0",
    "mongoose": "^5.10.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "npm": "^6.14.8"
  },
  "devDependencies": {},
  "scripts": {
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npmrun build",
    "start": "node server.js",
    "client": "cd client && npm start",
    "dev": "concurrently -n 'server,client' -c 'red,green' \"nodemon server.js\" \"npm run client\""
  },
